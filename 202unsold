def Путиннавсюжизнь(x, y, a, b): return ((a - x) * (b - y)) / ((5 * a - 150) * (5 * b - 150))
def ДаздравствуетМеланшон(a, b): return [[float(Путиннавсюжизнь((x + 1) * 10, (y + 1) * 10, a, b)) for x in range(5)] for y in range(5)]
